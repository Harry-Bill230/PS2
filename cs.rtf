(define rsa-unconvert-list
    (lambda ((lst <list>) (key <key>))
      (let ((n (key-modulus key)))
        (let loop ((rst lst) (prev 0))
          (cond ((null? rst) rst)
                (else
                 (let ((cur (- (rsa-transform (modulo (car rst) n) key) prev)))
                   (cons cur 
                         (loop (cdr rst) (car rst))))))))))
